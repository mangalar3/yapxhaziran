        var ilceLabel = document.createElement("label");
        ilceLabel.classList.add("left-margin");
        ilceLabel.innerHTML = ilce;
        ilceSecenek.appendChild(ilceLabel);

        ilcelerListesi.appendChild(ilceSecenek);
      }
