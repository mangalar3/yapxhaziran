<script>

  const illerContainer = document.getElementById('Iller');
  const ilcelerContainer = document.getElementById('Ilceler');

  iller.forEach(il => {
    const checkbox = document.createElement('input');
    checkbox.type = 'checkbox';
    checkbox.name = 'Iller';
    checkbox.value = il;
    checkbox.id = `il-${il}`;

    const label = document.createElement('label');
    label.htmlFor = `il-${il}`;
    label.appendChild(document.createTextNode(il));

    illerContainer.appendChild(checkbox);
    illerContainer.appendChild(label);
  });

  illerContainer.addEventListener('change', (e) => {
    ilcelerContainer.innerHTML = '';
    iller.forEach(il => {
      const ilCheckbox = document.getElementById(`il-${il}`);
      if (ilCheckbox.checked) {
        ilceler[il].forEach(ilce => {
          const checkbox = document.createElement('input');
          checkbox.type = 'checkbox';
          checkbox.name = 'Ilceler';
          checkbox.value = ilce;
          checkbox.id = `ilce-${ilce}`;

          const label = document.createElement('label');
          label.htmlFor = `ilce-${ilce}`;
          label.appendChild(document.createTextNode(ilce));

          ilcelerContainer.appendChild(checkbox);
          ilcelerContainer.appendChild(label);
        });
      }
    });
  });




