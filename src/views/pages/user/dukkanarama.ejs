  // İl seçimi değiştiğinde ilçe listesini güncelle
  illerListesi.addEventListener("change", function () {
    // Seçili illeri al
    var seciliIller = Array.from(illerListesi.querySelectorAll("input:checked")).map(function (checkbox) {
      return checkbox.value;
    });

    // İlçeler listesini temizle
    ilcelerListesi.innerHTML = "";

    // Seçili illerin ilçelerini ekle
    for (var i = 0; i < seciliIller.length; i++) {
      var seciliIl = seciliIller[i];
      var seciliIlIlceler = iller.find(function (il) {
        return il.il === seciliIl;
      }).ilceleri;

      // Seçili ilin adını bold (kalın) yazarak ekle
      var secilenIlLabel = document.createElement("li");
      secilenIlLabel.classList.add("selected-il");
      secilenIlLabel.classList.add("badge");
      secilenIlLabel.classList.add("badge-primary");
      secilenIlLabel.innerHTML = seciliIl;
      ilcelerListesi.appendChild(secilenIlLabel);

      // İlçeleri ekle
      for (var j = 0; j < seciliIlIlceler.length; j++) {
        var ilce = seciliIlIlceler[j];

        var ilceSecenek = document.createElement("p");
