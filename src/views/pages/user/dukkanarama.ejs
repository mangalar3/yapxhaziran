  function sendSelectedValues() {
    // Seçili illeri al
    var seciliIller = Array.from(illerListesi.querySelectorAll("input:checked")).map(function (checkbox) {
      return checkbox.value;
    });

    // İlçeleri toplamak için bir dizi oluştur
    var seciliIlceler = [];

    // Seçili illerin ilçelerini ekle
    for (var i = 0; i < seciliIller.length; i++) {
      var seciliIl = seciliIller[i];
      var seciliIlIlceler = Array.from(ilcelerListesi.querySelectorAll('input:checked')).map(function (checkbox) {
        return checkbox.value;
      });

      seciliIlceler.push(...seciliIlIlceler);
    }

    // Yalnızca benzersiz ilçeleri al
    seciliIlceler = [...new Set(seciliIlceler)];

    // Verileri JSON olarak göndermek için gerekli dizi oluştur
    var jsonData = [];

    // İl ve ilçe verilerini JSON nesnelerine dönüştürerek ekle
    for (var j = 0; j < seciliIller.length; j++) {
      var city = seciliIller[j];
      var country = seciliIlceler.filter(function (ilce) {
        return iller.find(function (il) {
          return il.il === city && il.ilceleri.includes(ilce);
        });
      });

      var data = {
        City: city,
        Country: country,
      };

      jsonData.push(data);
    }
    const bilgiler = {
      data: jsonData,
      aranan: "<%-aranan %>"
    }

    console.log(bilgiler);
    // AJAX ile POST isteği göndermek için gerekli parametreleri oluştur
    var xhr = new XMLHttpRequest();
    xhr.open("POST", "/FilterCityCountrySearch", true);
    xhr.setRequestHeader("Content-Type", "application/json");

    // AJAX isteği tamamlandığında gerçekleşecek işlev
    xhr.onload = function () {
      if (xhr.status === 200) {
        // İstek başarılı olduğunda yapılacak işlemler
        const data = JSON.parse(xhr.response)
        console.log(data);
        $("#dukkanlar").empty();
        data.forEach(Product => {
          $("#dukkanlar").append('<li class="col-6 col-md-3 col-wd-2gdot4 product-item"> <div class="product-item__outer h-100"> <div class="product-item__inner px-xl-4 p-3"> <div class="product-item__body pb-xl-2"> <div class="mb-2"><a href="/dukkanlar/'+ Product.dukkanurl +'" class="font-size-12 text-gray-5">'+ (Product.dukkanili).split(':')[1] + '(' +Product.dukkanilcesi +')' + '</a></div> <h5 class="mb-1 product-item__title"><a href="/dukkanlar/' +Product.dukkanurl +'" class="text-blue font-weight-bold">'+ Product.dukkanadi +'</a></h5> <div class="mb-2"> <a href="/dukkanlar/' +Product.dukkanurl +'" class="d-block text-center"><img class="img-fluid" src="/uploads/images/'+ Product.dukkanphoto +'" alt="'+Product.dukkanadi+'"></a> </div> </div> <div class="product-item__footer"> </div> </div> </div> </li>');
          

        });
      } else {
        // İstek başarısız olduğunda yapılacak işlemler
        console.error(xhr.statusText);
      }
    };

    // AJAX isteğini gönder
    xhr.send(JSON.stringify(bilgiler));
  }




  // Gönder butonuna tıklanınca sendSelectedValues işlevini çağır
  var gonderButton = document.getElementById("CityCountry");
  gonderButton.addEventListener("click", sendSelectedValues);



</script>
<script>
      // AJAX ile favorilere ekleme işlemi
      $('.add-to-favorites').click(function (e) {
        e.preventDefault();
        var link = $(this);
        var originalText = link.text();
        
        // Add loading animation
        link.html('<i class="fa fa-spinner fa-spin"></i> Yükleniyor...');
        
        $.ajax({
            url: link.attr('href'),
            type: 'GET',
            success: function (response) {
                link.html('<i class="fa fa-check-circle text-success"></i> Favorilere Eklendi').addClass('favorited');
            },
            error: function () {
                link.text(originalText); // Restore original text if there is an error
            }
        });
    });
</script>