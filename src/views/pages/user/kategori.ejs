    <script>


      $(document).ready(function () {
        // Favori ekleme işlemi
        $(document).on('click', '.add-to-favorites', function (e) {
          e.preventDefault();
          var link = $(this);
          var originalText = link.text();
          var href = link.attr('href');

          // Add loading animation
          link.html('<i class="fa fa-spinner fa-spin"></i> Yükleniyor...');

          $.ajax({
            url: href,
            type: 'GET',
            success: function (response) {
              if (response.status) {
                // Favori eklendi, bağlantıyı güncelle
                link.html('<i class="fa fa-times-circle text-red"></i> Favorilerden Kaldır').addClass('favorited');
                link.attr('href', '/isteklistesiremove/' + extractProductUrl(href));
              } else {
                // Favori kaldırıldı, bağlantıyı güncelle
                link.html('<i class="fa fa-heart"></i> Favorilere Ekle').removeClass('favorited');
                link.attr('href', '/isteklistesiekle/' + extractProductUrl(href));
              }
            },
            error: function () {
              link.text(originalText); // Hata durumunda orijinal metni geri yükle
            }
          });
        });

        // Yardımcı fonksiyon: Ürün URL'sini çıkarma
        function extractProductUrl(url) {
          var parts = url.split('/');
          return parts[parts.length - 1];
        }
      });
    </script>
